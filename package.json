{
  "name": "react-native-zetic",
  "version": "0.1.0",
  "description": "react-native module to use zetic service",
  "main": "src/index",
  "author": " <> ()",
  "license": "UNLICENSED",
  "homepage": "#readme",
  "create-react-native-library": {
    "type": "legacy-module",
    "languages": "kotlin-swift",
    "version": "0.49.7"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "commitlint": "^19.8.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "react-native": "^0.79.0",
    "release-it": "^18.1.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "on-device",
    "ai"
  ],
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  }
}
